<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g="urn:import:com.google.gwt.user.client.ui"
             xmlns:m="urn:import:gwt.material.design.client.ui"
             xmlns:widget="urn:import:gmd.addins.demo.client.widget"
             xmlns:ma="urn:import:gwt.material.design.addins.client">

    <m:MaterialPanel>
        <g:HTMLPanel>
            <pre class="xml" style="height: 40px;overflow: hidden">
    &lt;ma:docviewer.MaterialDocViewer />
            </pre>
        </g:HTMLPanel>
        <widget:CodeSection sectionTitle="Introduction" description="This component is an iframe that wraps your document using Google Doc Viewer. It's good for public document.">
            <m:MaterialRow paddingLeft="20">
                <ma:docviewer.MaterialDocViewer width="100%" height="600px" url="http://www.africau.edu/images/default/sample.pdf" />
            </m:MaterialRow>
        </widget:CodeSection>
    </m:MaterialPanel>
</ui:UiBinder>
